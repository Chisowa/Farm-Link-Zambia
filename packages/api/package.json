{
  "name": "@farm-link/api",
  "version": "0.1.0",
  "private": true,
  "description": "Shared tRPC router and Zod schemas",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./routers": {
      "import": "./dist/routers/index.js",
      "types": "./dist/routers/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@trpc/server": "^10.45.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "typescript": "^5.3.2",
    "@types/node": "^20.10.6"
  }
}
